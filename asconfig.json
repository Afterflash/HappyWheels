{
	"config": "air",
	"compilerOptions": {
		"source-path": [
			"src"
		],
		"library-path": [
			"core/libs"
		],
		"output": "core/output/happy_wheels.swf",
		"default-background-color": "#3d88c7",
		"default-frame-rate": 30,
